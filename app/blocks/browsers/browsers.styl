img
	width 40%
.browsers
	&__images
		position relative
		margin-top 30px
		height 298px

	&__image-left
		position absolute
		top 51px
		left 15%
	&__image-right
		position absolute
		top 51px 
		right 15%
	&__image-center
		position absolute
		z-index 1
		left 50%
		transform translateX(-50%)
