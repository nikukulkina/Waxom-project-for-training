img
	width 60%
.browsers
	&__images
		position relative
		margin-top 30px
		height 402px

	&__image-left
		position absolute
		top 17%
	&__image-right
		position absolute
		top 17%
		right 1%
	&__image-center
		position absolute
		z-index 1
		left 50%
		transform translateX(-50%)
